{
  "presets": [
    [
        "@babel/preset-env",
        {
          "targets": {
            "node": "16"
          },
          "useBuiltIns": "usage",
          "exclude": "/node_modules/"
        }
      ],
    "@babel/preset-typescript"
  ],
  "plugins": [
    "@babel/plugin-proposal-class-properties"
  ]
}